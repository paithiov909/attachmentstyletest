  "devDependencies": {
    "cpy-cli": "4.1.0",
    "fibers": "5.0.1",
    "husky": "8.0.1",
    "live-server": "1.2.2",
    "mkdirp": "1.0.4",
    "nodemon": "2.0.18",
    "prettier": "2.8.8",
